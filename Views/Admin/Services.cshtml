@model List<Services>
@{
    ViewData["Title"] = "Manage";
}
<a href="~/Admin/AddServices">Add Service</a>
<h1>Sevices</h1>
<div class="row">
    <div class="col">
        @if (Model != null) 
        {
            <table class="table table-hover">
                <tr>
                    <th>Id</th>
                    <th>Title</th>
                    <th>Action </th>
                </tr>

                @foreach (var task in Model)
                {
                    <tr>
                        <td>@task.ServiceSysId</td>
                        <td>@task.ServiceTitle</td>
                        <td>
                            <a href="">Activate</a> 
                        
                        </td>
                    </tr>
                }
            </table>
        }
    </div>
</div>
