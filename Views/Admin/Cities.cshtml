@model List<Cities>
@{
    ViewData["Title"] = "Manage Tasks : List";
}
<a href="\Admin\AddCities">Add City</a>
<h1>Cities</h1>
<div class="row">
    <div class="col">
        @if (Model != null) 
        {
            <table class="table table-hover">
                <tr>
                    <th>Code</th>
                    <th>Name</th>
                    <th>Action </th>
                </tr>

                @foreach (var task in Model)
                {
                    <tr>
                        <td>@task.CityCode</td>
                      
                        <td>@task.CityName</td>
                        <td>
                            <a href="">Activate</a> 
                            <a href="/Admin/DeleteUser1?cityCode=@task.CityCode">Delete</a>
                        </td>
                    </tr>
                }
            </table>
        }
    </div>
</div>

  