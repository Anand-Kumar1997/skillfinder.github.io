@model List<ServiceProviders>
    
@{
    ViewData["Title"] = "Manage";
}
<a href="\Admin\AddProviders">Add Provider</a>
<h1>Providers</h1>
<div class="row">
    <div class="col">
        @if (Model != null) 
        {
            <table class="table table-hover">
                <tr>
                    <th>Id</th>
                    <th>Name</th>
                    <th>Contact</th>
                    <th>DOB</th>
                    <th>Action </th>
                </tr>

                @foreach (var task in Model)
                {
                    <tr>
                        <td>@task.ServiceProviderSysId</td>
                        <td>@task.Name</td>
                        <td>@task.ContNo</td>
                        <td>@task.Dob</td>
                        <td>
                            <a href="">Edit</a> 
                            <a href="/Admin/Delete?serviceProviderSysId=@task.ServiceProviderSysId">Delete</a>
                        </td>
                    </tr>
                }
            </table>
        }
    </div>
</div>
